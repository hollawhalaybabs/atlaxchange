
<templates>
    <t t-name="atlaxchange_ledger.Atlaxchange_Dashboard">
        <div class="o_hr_dashboard">
            <div class="o_hr_dashboard_section">
                <h3>Total Transactions</h3>
                <span t-esc="data.total_transactions_count or 0"/>
            </div>
            <t t-foreach="data.sections" t-as="section">
                <div class="o_hr_dashboard_section">
                    <h3 t-esc="section.title"/>
                    <span t-esc="section.value"/>
                </div>
            </t>
        </div>
    </t>
</templates>
